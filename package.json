{
  "name": "inner-circle-compensations-ui",
  "version": "0.0.1",
  "private": true,
  "homepage": "/compensations",
  "scripts": {
    "start": "npm run create-config:dev && vite --host",
    "start:local": "npm run create-config:local && vite --host",
    "start-local-env": "vite --host --mode local-env",
    "build": "tsc && vite build",
    "docker:build": "docker build -t inner-circle-compensations-ui .",
    "docker:run": "docker run -p 4455:80 inner-circle-compensations-ui",
    "lint": "run-s lint-ts lint-eslint",
    "lint-eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-ts": "tsc --noEmit",
    "preview": "vite preview",
    "create-config:dev": "node local-config-builder dev",
    "create-config:local": "node local-config-builder local",
    "test": "run-s cypress:run:component",
    "cypress:open": "cypress open",
    "cypress:open:component": "cypress open --component --browser chrome",
    "cypress:run:component": "cypress run --component --browser chrome",
    "cypress:open:e2e": "cypress open --e2e --browser chrome --config-file cypress.config.prod.ts",
    "cypress:run:e2e": "cypress run --e2e --config-file cypress.config.prod.ts",
    "cypress:open:e2e:local-env": "cypress open --e2e --browser chrome --config-file cypress.config.local-env.ts",
    "cypress:run:e2e:local-env": "cypress run --e2e --config-file cypress.config.local-env.ts"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@originjs/vite-plugin-federation": "^1.3.9",
    "@tourmalinecore/react-tc-auth": "^2.0.1",
    "axios": "^1.6.7",
    "history": "^5.3.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.2.2",
    "react-toastify": "^11.0.3",
    "use-react-router-breadcrumbs": "^4.0.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.84.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}